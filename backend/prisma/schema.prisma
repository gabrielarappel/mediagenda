generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Medico {
  id             Int        @id @default(autoincrement())
  nome           String
  crm            String
  especialidade  String
  consultas      Consulta[]
}

model Consulta {
  id        Int      @id @default(autoincrement())
  medicoId  Int
  paciente  String
  dataHora  DateTime
  medico    Medico   @relation(fields: [medicoId], references: [id])
}
